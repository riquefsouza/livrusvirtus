#>+ 11
podbc.all_cpp.cpp: $(srcdir)/Makefile.in $(srcdir)/main.cpp  
	@echo 'creating podbc.all_cpp.cpp ...'; \
	rm -f podbc.all_cpp.files podbc.all_cpp.final; \
	echo "#define KDE_USE_FINAL 1" >> podbc.all_cpp.final; \
	for file in main.cpp ; do \
	  echo "#include \"$$file\"" >> podbc.all_cpp.files; \
	  test ! -f $(srcdir)/$$file || egrep '^#pragma +implementation' $(srcdir)/$$file >> podbc.all_cpp.final; \
	done; \
	cat podbc.all_cpp.final podbc.all_cpp.files  > podbc.all_cpp.cpp; \
	rm -f podbc.all_cpp.final podbc.all_cpp.files

#>+ 3
