							String: sISBN
							String: sTitulo
							Number: nEdicao
							Number: nAnoPubli
							Number: nVolume
							Number: nCodEditora
							Number: nCodIdioma
							Number: nNPaginas
							Number: nPreco
							Number: nQtdEstoque
							String: sRetorno
						Static Variables
						Local variables
							String: sCons
						Actions
							If sTipo = 'S'
								Set sCons = "select l.isbn as ISBN, l.titulo as Titulo, l.edicao as Edicao,
										l.anopubli as AnoPublicacao, l.codeditora as CodEditora, l.volume as Volume,
										e.editora as Editora, l.codidioma as CodIdioma, i.idioma as Idioma,
										l.npaginas as NumPaginas, l.preco as Preco, l.qtdestoque as Estoque into " || sRetorno ||
										" from Livro l, editora e, idioma i
										where l.codeditora=e.codeditora
										and l.codidioma=i.codidioma"
								If sISBN != ''
									Set sCons = sCons || " and l.isbn='" || sISBN || "'"
								If sTitulo != ''
									Set sCons = sCons || " and l.titulo like '%" || sTitulo || "%'"
							Else If sTipo = 'I'
								Set sCons = "insert into Livro(isbn, titulo, edicao, anopubli, codeditora,
										volume, codidioma,npaginas, preco, qtdestoque) values('" ||
										sISBN || "','" || sTitulo || "'," || SalNumberToStrX( nEdicao, 0 ) || "," || 
										SalNumberToStrX( nAnoPubli, 0 ) || "," || SalNumberToStrX( nCodEditora, 0 ) || "," || 
										SalNumberToStrX( nVolume, 0 ) || "," || SalNumberToStrX( nCodIdioma, 0 ) || "," || 
										SalNumberToStrX( nNPaginas, 0 ) || "," || 
										Rotinas.VirgulaParaPonto( SalNumberToStrX( nPreco, 2 )) || "," || 
										SalNumberToStrX( nQtdEstoque, 0 ) || ")"
