					Function: modulo11
						Description:
						Returns
							Number:
						Parameters
							String: svalor
							Number: ndig
						Static Variables
						Local variables
							Number: ncont
							Number: nsoma
							Number: nvalor
							Number: npos
							String: sres
							String: slocal
						Actions
							Set ncont = 0
							While ncont < 11
								Set slocal = SalStrMidX( svalor,ncont,1) || slocal
								Set ncont = ncont + 1
							Set nsoma = 0
							If ndig=1 
								Set ncont = 2
							Else
								Set ncont = 1
							Set npos = 2
							While ncont < 11
								Set sres = SalStrMidX(slocal,ncont,1)
								Set nvalor = SalStrToNumber( sres )
								Set nsoma = nsoma + (nvalor * npos)
								Set ncont = ncont + 1
								Set npos = npos + 1
							Set nsoma = (11 - SalNumberMod( nsoma, 11 ))
							If nsoma > 9
								Return 0
							Else
								Return nsoma
					Function: ValidaCPF
						Description:
						Returns
							Boolean:
						Parameters
							String: cpf
						Static Variables
						Local variables
							String: scpf
						Actions
							If SalStrLength( cpf ) = 14
								Set scpf = SalStrMidX( cpf, 0, 3 ) || SalStrMidX( cpf, 4, 3 ) || 
										           SalStrMidX( cpf, 8, 3 ) || SalStrMidX( cpf, 12, 2 )
								If ( (modulo11(scpf,1)=SalStrToNumber( SalStrMidX( cpf,12,1 ) ) ) AND
										  (modulo11(scpf,2)=SalStrToNumber( SalStrMidX( cpf,13,1 ) ) ) )
									Return TRUE
								Else
									Return FALSE
							Else
								Return FALSE
					Function: ValidaISBN
						Description:
						Returns
							Boolean:
						Parameters
							String: isbn
						Static Variables
						Local variables
							String: sisbn
							String: snum
							String: sdigito
							Number: nvalor
							Number: nsoma
							Number: ncont
						Actions
							If SalStrLength( isbn ) = 13
								Set sdigito = SalStrMidX(isbn, SalStrLength(isbn)-1, 1)
								Set sisbn = ''
								Set ncont = 1
								While ncont <= ( SalStrLength( isbn ) - 1)
									If SalStrMidX( isbn, ncont-1, 1 ) != '-'
										Set sisbn = sisbn || SalStrMidX( isbn, ncont-1 ,1 )
									!
									Set ncont = ncont +1
								!
								If sdigito = 'X'
									Set nsoma = 10
								Else
									Set nvalor = SalStrToNumber( sdigito )
									Set nsoma = nvalor
								Set ncont = 1
								While ncont <= 9
									Set snum = SalStrMidX(sisbn, ncont-1, 1)
									Set nvalor = SalStrToNumber( snum )
									Set nsoma = nsoma + (nvalor * (11 - ncont))
									!
									Set ncont = ncont +1
								!
								If SalNumberMod( nsoma, 11) = 0
									Return TRUE
								Else
									Return FALSE
							Else
								Return FALSE
