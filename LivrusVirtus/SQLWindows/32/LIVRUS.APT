					Function: Venda
						Description:
						Returns
							String:
						Parameters
							String: sTipo
							String: sISBN
							String: sCPF
							Date/Time: dDataVenda
							Number: nPrecoVenda
							Number: nQtdEstoque
							String: sRetorno
						Static Variables
						Local variables
							String: sCons
						Actions
							If (sTipo='S') OR (sTipo='P') 
								If sTipo = 'S'
									Set sCons = "select v.datavenda as DataVenda, v.cpf as CPF, c.nome as Cliente,
											v.isbn as ISBN,l.Titulo as Livro, l.preco as Preco into " || sRetorno ||
											" from Venda v, Livro l, Cliente c
											where v.isbn=l.isbn and v.cpf=c.cpf"
								Else If sTipo = 'P'
									Set sCons = "select sum(v.precovenda) as PrecoTotal into " || sRetorno ||  
											" from Venda v, Livro l, Cliente c
											where v.isbn=l.isbn and v.cpf=c.cpf"
								If sISBN!=''
									Set sCons=sCons || " and v.isbn='" || sISBN || "'"
								If sCPF!=''
									Set sCons=sCons || " and v.cpf='" || sCPF || "'"
								If dDataVenda!=DATETIME_Null
									Set sCons=sCons || " and v.datavenda='" || 
											SalFmtFormatDateTime( dDataVenda, 'MM/dd/yyyy hh:mm:ss' ) || "'"
							Else If sTipo = 'I'
								Set sCons = "insert into Venda(isbn, cpf, datavenda, precovenda) values('" ||
										sISBN || "','" || sCPF || "','" || 
										SalFmtFormatDateTime( dDataVenda, 'MM/dd/yyyy hh:mm:ss' ) || "'," || 
										Rotinas.VirgulaParaPonto( SalNumberToStrX( nPrecoVenda , 2 ) ) || ")"
							Else If sTipo = 'U'
								Set sCons = "update Livro set qtdestoque=" || SalNumberToStrX( nQtdEstoque, 0 ) || 
										" where isbn='" || sISBN || "'"
							Else If sTipo = 'D'
								Set sCons = "delete from Venda where isbn='" || sISBN ||
										"' and cpf='" || sCPF || "' and datavenda='" || 
										SalFmtFormatDateTime( dDataVenda, 'MM/dd/yyyy hh:mm:ss' ) || "'"
							!
							Return sCons
