			Function: InformaAssuntoAutor
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Call Rotinas.AdicionaValoresLista( lstCodAssuntos, lstAssuntos, 
							ConsSQL.LivroAss('S1',edtCodigo,NUMBER_Null), ConsSQL.LivroAss('S2',edtCodigo,NUMBER_Null))
					Call Rotinas.AdicionaValoresLista( lstCodAutores, lstAutores, 
							ConsSQL.LivroAut('S1',edtCodigo,NUMBER_Null), ConsSQL.LivroAut('S2',edtCodigo,NUMBER_Null))
			Function: InformaIdioma
				Description:
				Returns
				Parameters
				Static Variables
				Local variables
				Actions
					Call SalListSelectString( cmbCodIdioma, 0, SalNumberToStrX( nCodIdioma, 0 ) )
					Call SalListSetSelect( cmbIdioma, SalListQuerySelection( cmbCodIdioma ) )
			Function: SalvaLista
				Description:
				Returns
				Parameters
					String: sOpcao
				Static Variables
				Local variables
					Number: nCont
				Actions
					If sOpcao='A'
						Call Rotinas.AtualizaDados( hSqlArquiva, ConsSQL.LivroAss('D',edtCodigo, NUMBER_Null))
						Set nCont = 0
						While nCont < SalListQueryCount( lstCodAssuntos )
							Call Rotinas.AtualizaDados( hSqlArquiva, ConsSQL.LivroAss('I',edtCodigo, 
									SalStrToNumber( SalListQueryTextX( lstCodAssuntos, nCont ) )))
							Set nCont = nCont + 1
					Else If sOpcao='B'
						Call Rotinas.AtualizaDados( hSqlArquiva, ConsSQL.LivroAut('D',edtCodigo, NUMBER_Null))
						Set nCont = 0
						While nCont < SalListQueryCount( lstCodAutores )
							Call Rotinas.AtualizaDados( hSqlArquiva, ConsSQL.LivroAut('I',edtCodigo, 
									SalStrToNumber( SalListQueryTextX( lstCodAutores, nCont ) )))
							Set nCont = nCont + 1
