					Function: Assunto
						Description:
						Returns
							String:
						Parameters
							String: sTipo
							Number: nCodigo
							String: sAssunto
							String: sRetorno
						Static Variables
						Local variables
							String: sCons
						Actions
							If sTipo = 'N'
								Set sCons = "select max(codassunto)+1 from Assunto into " || sRetorno
							Else If sTipo = 'S'
								Set sCons = "select a.codassunto as Codigo, a.assunto as Autor into " || sRetorno || " from Assunto a 
										where a.codassunto=a.codassunto"
								If nCodigo != NUMBER_Null
									Set sCons = sCons || " and a.codassunto=" || SalNumberToStrX( nCodigo, 0 )
								If sAssunto != ''
									Set sCons = sCons || " and a.assunto like '%" || sAssunto || "%'"
							Else If sTipo = 'C'
								Set sCons = "select a.codassunto, a.assunto into " || sRetorno || " from Assunto a
										where a.codassunto=a.codassunto"
								If sAssunto != ''
									Set sCons = sCons || " and a.assunto like '%" || sAssunto || "%'"
							Else If sTipo = 'I'
								Set sCons = "insert into Assunto(codassunto, assunto) values(" ||
										SalNumberToStrX( nCodigo, 0 ) || ",'" || sAssunto || "')"
							Else If sTipo = 'U'
								Set sCons = "update Assunto set assunto='" || sAssunto || 
										"' where codassunto=" || SalNumberToStrX( nCodigo, 0 )
							Else If sTipo = 'D'
								Set sCons = "delete from Assunto where codassunto=" || SalNumberToStrX( nCodigo, 0 )
							!
							Return sCons
